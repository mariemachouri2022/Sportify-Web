{% extends 'base.html.twig' %}

{% block title %}Score{% endblock %}

{% block body %}
    <h1>Score</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Idscore</th>
                <td>{{ score.idscore }}</td>
            </tr>
            <tr>
                <th>Competitionid</th>
                <td>{{ score.competitionid }}</td>
            </tr>
            <tr>
                <th>Winnerid</th>
                <td>{{ score.winnerId }}</td>
            </tr>
            <tr>
                <th>Loserid</th>
                <td>{{ score.loserid }}</td>
            </tr>
            <tr>
                <th>Equipe1score</th>
                <td>{{ score.equipe1score }}</td>
            </tr>
            <tr>
                <th>Equipe2score</th>
                <td>{{ score.equipe2score }}</td>
            </tr>
            <tr>
                <th>Resultat</th>
                <td>{{ score.resultat }}</td>
            </tr>
            <tr>
                <th>Reclamation</th>
                <td>{{ score.reclamation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_score_index') }}">back to list</a>

    <a href="{{ path('app_score_edit', {'idscore': score.idscore}) }}">edit</a>

    {{ include('score/_delete_form.html.twig') }}
