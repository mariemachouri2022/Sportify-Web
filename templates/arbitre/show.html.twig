{% extends 'equipe/base.html.twig' %}

{% block title %}Arbitre{% endblock %}

{% block body %}
    <h1>Arbitre</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdArbitre</th>
                <td>{{ arbitre.idArbitre }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ arbitre.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ arbitre.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ arbitre.email }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ arbitre.phone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_arbitre_index') }}">back to list</a>

    <a href="{{ path('app_arbitre_edit', {'idArbitre': arbitre.idArbitre}) }}">edit</a>

    {{ include('arbitre/_delete_form.html.twig') }}
{% endblock %}
